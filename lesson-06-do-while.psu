--- simple for ---
FOR i=1 TO 10 DO 
   print "hello world"
ENDFOR

--- with a step ---
FOR i=1 TO 10 STEP 2 DO 
   print "hello world"
ENDFOR

--- while loop --- 
DEFINE x = 0
WHILE (x < 5) DO 
    print "while is working"
    x = x + 1
ENDWHILE

